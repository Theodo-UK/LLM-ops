datasource client {
  provider = "sqlite"
  url      = "file:dev.db"
}

generator client {
  provider = "prisma-client-py"
  interface = "asyncio"
  recursive_type_depth = 5
}

model Log {
  id            String   @default(cuid()) @id
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt
  input         String
  output        String
  token_count   Int
}
